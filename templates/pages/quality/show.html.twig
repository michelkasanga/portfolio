{% block title %}Quality{% endblock %}

{% block body %}
    <h1>Quality</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ quality.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ quality.name }}</td>
            </tr>
            <tr>
                <th>CreatedAt</th>
                <td>{{ quality.createdAt ? quality.createdAt|date('Y-m-d H:i:s') : '' }}</td>
